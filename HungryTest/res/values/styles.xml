<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="CartActionbar" parent="@android:style/Theme.Holo.Light">

        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:actionBarStyle">@style/CartActionbarBlackground </item>
<item name="android:actionBarTabTextStyle">@style/CartActionbarTabText</item> 
<item name="android:actionBarTabStyle">@style/CartActionbarBackground</item>
<item name="android:textSize">20sp</item>
<item name="android:homeAsUpIndicator">@drawable/back</item>
    </style>

    <style name="CartActionbarBlackground" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@color/blackground</item>
           <item name="android:gravity">center_horizontal</item>
           <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>

    </style>
        <style name="MyActionBarTitleText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#fff</item>
    </style>
    <style name="CartActionbarTabText" parent="@android:style/Widget.ActionBar.TabText">
        <item name="android:textColor">@drawable/selector_cart_tab</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
  <style name="CartActionbarBackground" parent="@android:style/Widget.ActionBar.TabBar" >
      <item name="android:background">@drawable/selector_cart_tab_background</item>
      <item name="android:gravity">center_horizontal</item>
      <item name="android:alignmentMode" >alignBounds</item>
  </style>

  
  <style name="Universal" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/CartActionbarBlackground 
</item>
<item name="android:textSize">20sp</item>
<item name="android:homeAsUpIndicator">@drawable/back</item>
</style>


  
  
  
  
  
